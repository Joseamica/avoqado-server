    "test:utils": "jest tests/unit/utils",
    "test:watch": "jest --watch",
    "test:watch:unit": "jest tests/unit --watch",
    "test:watch:api": "jest tests/api-tests --watch",
    "test:coverage": "jest --coverage",
    "test:coverage:unit": "jest tests/unit --coverage",
    "test:coverage:api": "jest tests/api-tests --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false --passWithNoTests",
