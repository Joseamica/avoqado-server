{
  "name": "avoqado-server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npx prisma generate && tsc && tsc-alias",
    "start": "node -r tsconfig-paths/register dist/src/server.js",
    "dev": "nodemon --watch src --ext ts,json --exec tsx src/server.ts | pino-pretty",
    "dev:simple-logging": "SIMPLE_LOGGING=true nodemon --watch src --ext ts,json --exec tsx src/server.ts",
    "dev:slow": "nodemon --watch src --ext ts,json --exec ts-node -r tsconfig-paths/register src/server.ts | pino-pretty",
    "kill-port": "bash scripts/kill-port.sh",
    "predeploy:backend": "npx prisma generate && npx prisma migrate deploy ",
    "migrate": "prisma migrate dev",
    "seed": "npx prisma db seed && npx ts-node -r tsconfig-paths/register scripts/sync-features-to-stripe.ts",
    "studio": "prisma studio",
    "studio:render": "USE_RENDER_DB=true npx prisma studio",
    "test": "jest",
    "test:unit": "NODE_OPTIONS='--max-old-space-size=4096' jest tests/unit --maxWorkers=2",
    "test:integration": "jest --selectProjects=integration",
    "test:api": "jest tests/api-tests",
    "test:workflows": "jest tests/workflows",
    "test:dashboard": "jest tests/unit/controllers/dashboard tests/unit/services/dashboard tests/api-tests/dashboard tests/workflows/dashboard-management",
    "test:pos-sync": "jest tests/unit/controllers/pos-sync tests/unit/services/pos-sync tests/api-tests/pos-sync tests/workflows/pos-sync",
    "test:public": "jest tests/unit/controllers/public tests/api-tests/public",
    "test:tpv": "jest tests/unit/controllers/tpv tests/api-tests/tpv",
    "test:orders": "jest tests/api-tests/orders tests/workflows/orders",
    "test:auth": "jest tests/workflows/auth tests/api-tests/dashboard/auth.api.test.ts",
    "test:communication": "jest tests/unit/communication",
    "test:middlewares": "jest tests/unit/middlewares",
    "test:utils": "jest tests/unit/utils",
    "test:watch": "jest --watch",
    "test:watch:unit": "jest tests/unit --watch",
    "test:watch:api": "jest tests/api-tests --watch",
    "test:coverage": "jest --coverage",
    "test:coverage:unit": "jest tests/unit --coverage",
    "test:coverage:api": "jest tests/api-tests --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false --passWithNoTests",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "format": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "check:unused": "unimported",
    "check:dead-code": "knip",
    "check:all": "npm run check:unused && npm run check:dead-code",
    "update:unused-ignore": "node scripts/update-unused-ignore.js",
    "pre-deploy": "bash scripts/pre-deploy-check.sh",
    "pre-migrate": "npx ts-node -r tsconfig-paths/register scripts/pre-migration-check.ts",
    "blumon:help": "npx ts-node scripts/blumon-help.ts",
    "blumon:auth": "npx ts-node -r tsconfig-paths/register scripts/blumon-authenticate-master.ts",
    "blumon:session": "npx ts-node -r tsconfig-paths/register scripts/create-test-checkout-session.ts",
    "blumon:sessions": "npx ts-node -r tsconfig-paths/register scripts/list-active-sessions.ts",
    "blumon:webhook": "npx ts-node -r tsconfig-paths/register scripts/test-webhook-simulator.ts",
    "blumon:merchant": "npx ts-node -r tsconfig-paths/register scripts/check-blumon-merchant.ts",
    "blumon:mock": "npx ts-node -r tsconfig-paths/register scripts/test-blumon-mock.ts",
    "blumon:flow": "npx ts-node -r tsconfig-paths/register scripts/test-blumon-checkout-flow.ts",
    "dev:dashboard": "echo '\nðŸŽ¯ Session Dashboard: http://localhost:3000/sdk/sessions/dashboard\n' && open http://localhost:3000/sdk/sessions/dashboard",
    "dev:clean-sessions": "npx ts-node -r tsconfig-paths/register scripts/cleanup-old-sessions.ts",
    "dev:logs": "tail -f logs/$(ls -t logs/development*.log | head -1)",
    "sdk:test": "npx ts-node -r tsconfig-paths/register scripts/test-ecommerce-merchant-endpoints.ts",
    "sdk:errors": "npx ts-node -r tsconfig-paths/register scripts/test-error-parser.ts"
  },
